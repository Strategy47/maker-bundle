<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

        <services>
            <defaults public="false" />

            <service id="maker.maker.make_authenticator" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeAuthenticator">
                <argument type="service" id="maker.file_manager" />
                <argument type="service" id="maker.security_config_updater" />
                <argument type="service" id="maker.generator" />
                <argument type="service" id="maker.doctrine_helper" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_command" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeCommand">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_controller" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeController">
                <argument type="service" id="maker.file_manager" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_crud" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeCrud">
                <argument type="service" id="maker.doctrine_helper" />
                <argument type="service" id="maker.renderer.form_type_renderer" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_docker_database" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeDockerDatabase">
                <argument type="service" id="maker.file_manager" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_entity" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeEntity">
                <argument type="service" id="maker.file_manager" />
                <argument type="service" id="maker.doctrine_helper" />
                <argument>%kernel.project_dir%</argument>
                <argument type="service" id="maker.generator" />
                <argument type="service" id="maker.entity_class_generator" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_fixtures" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeFixtures">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_form" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeForm">
                <argument type="service" id="maker.doctrine_helper" />
                <argument type="service" id="maker.renderer.form_type_renderer" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_functional_test" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeFunctionalTest">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_message" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeMessage">
                <argument type="service" id="maker.file_manager" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_messenger_middleware" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeMessengerMiddleware">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_registration_form" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeRegistrationForm">
                <argument type="service" id="maker.file_manager" />
                <argument type="service" id="maker.renderer.form_type_renderer" />
                <argument type="service" id="router" />
                <argument type="service" id="maker.doctrine_helper" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_reset_password" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeResetPassword">
                <argument type="service" id="maker.file_manager" />
                <argument type="service" id="maker.doctrine_helper" />
                <argument type="service" id="maker.entity_class_generator" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_serializer_encoder" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeSerializerEncoder">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_serializer_normalizer" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeSerializerNormalizer">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_subscriber" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeSubscriber">
                <tag name="maker.command" />
                <argument type="service" id="maker.event_registry" />
            </service>

            <service id="maker.maker.make_twig_extension" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeTwigExtension">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_test" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeTest">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_unit_test" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeUnitTest">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_validator" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeValidator">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_voter" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeVoter">
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_user" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeUser">
                <argument type="service" id="maker.file_manager" />
                <argument type="service" id="maker.user_class_builder" />
                <argument type="service" id="maker.security_config_updater" />
                <argument type="service" id="maker.entity_class_generator" />
                <tag name="maker.command" />
            </service>

            <service id="maker.maker.make_migration" class="Mamizo\Bundle\GeneratorBundle\Maker\MakeMigration">
                <argument>%kernel.project_dir%</argument>
                <tag name="maker.command" />
            </service>
        </services>
</container>
